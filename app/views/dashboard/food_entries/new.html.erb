<% meta title: "Dashboard - New Food Entry" %>
<% @page_title = "Food Entries" %>

<section class="bg-white rounded-2 shadow p-4">
  <div class="row gy-0">
    <div class="col-xxl-16">
      <h2 class="h5 mb-3 mb-md-4">New food entry</h2>
      <%= render "layouts/alerts" %>
      <%= simple_form_for @food_entry, url: dashboard_food_entries_path, validate: true do |form| %>
        <%= render partial: "form", locals: {form: form} %>
      <% end %>
    </div>
  </div>
</section>
